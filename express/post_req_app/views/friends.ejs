<h1>Friends</h1>

<% friendsArray.forEach(friend => { %>
    <p><%= friend.name %> , <%= friend.age %></p>
<% }) %>

<form action="/addfriend " method="POST">
    <input type="text" name="name" placeholder="name">
    <!-- <input type="text" name="age" placeholder="age"> -->
    <button> Make a new friend</button>
</form>